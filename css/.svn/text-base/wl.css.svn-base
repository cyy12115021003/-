ul.imgList li{display:inline-block;width:16%;padding:5px;cursor:pointer;}
ul.imgList li:hover, .hoveredLi{box-shadow:0 0 2px 3px #00FCFF;}
/*当鼠标悬浮到div上方是，给div添加阴影*/
.imgList li img{width:100%;height:60%;}
.xuanfu{
    position: absolute;
    width: 40%;
    left: 30%;
    top: 30%;
    background-color: rgba(255,255,255,.9);
    padding: 10px;
    border-radius: 5px;
    text-align: center;
    font-size: 16px;
    border: 2px solid #000;
    color: #000;
}

.circleone{width:10px;height: 10px;border-radius: 50%; -moz-border-radius: 50%;
    -webkit-border-radius: 50%;background-color:#FFCA6A;}
.circletwo{width:10px;height: 10px;border-radius: 50%; -moz-border-radius: 50%;
    -webkit-border-radius: 50%;background-color:#6ADDFF;}
    
.moreli{
	position: absolute;top: 100%;left: 0;right: 0;
	border: 1px solid #3f6a9d;
	border-bottom: none;	
	list-style: none;	
	background-color: rgba(17,54,96,0.9);
	display: none;
}
.moreli li{
	border-bottom: 1px solid #3f6a9d;
	text-align: center;
	cursor: pointer;
	color: #CAEDFF;
	font-size: 25px;
}

.shanshan{	    
    animation: flash .4s linear infinite;
}
 
@keyframes flash{
    from {  opacity: 0;  }
    to {  opacity: 1;  }
}
.scrollList{  padding: 15px;  }
.scrollList li{font-size: 15px;line-height:26px;word-break:break-all;padding:6px 0px;}
/*地图颜色代表  图例*/
.lenged{width: 25px;height: 10px;}
.mapColor1{background-color: #524D15;}
.mapColor2{background-color:#75AC47;}
.mapColor3{background-color:#C0E586;}
.circlemap{border-radius: 50%; -moz-border-radius: 50%;-webkit-border-radius: 50%;background-color:#fff;}
